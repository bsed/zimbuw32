CLASS OverController

  FUNC $over(ZUT.Event event) bool
    IO.print(">>>over")
    RETURN FALSE
  }

  FUNC $out(ZUT.Event event) bool
    IO.print("out<<<")
    RETURN FALSE
  }
}
